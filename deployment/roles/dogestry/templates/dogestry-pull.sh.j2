#!/usr/bin/env bash

echo "Pulling docker image ${1} from S3 bucket {{ aws.dockerImageBucket }}"

export AWS_ACCESS_KEY={{ aws.accessKeyId }}
export AWS_SECRET_KEY={{ aws.secretKey }}
export DOCKER_HOST=tcp://localhost:4243
dogestry pull s3://{{ aws.dockerImageBucket }}?region={{ aws.dockerBucketRegion }} $1